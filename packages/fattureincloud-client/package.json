{
  "name": "@jackdbd/fattureincloud-client",
  "version": "1.0.0",
  "description": "Unofficial API client for FattureInCloud",
  "author": "Giacomo Debidda <giacomo@giacomodebidda.com> (https://giacomodebidda.com/)",
  "license": "MIT",
  "private": true,
  "keywords": [
    "fattureincloud",
    "invoicing"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jackdbd/calderone"
  },
  "homepage": "https://github.com/jackdbd/calderone/tree/main/packages/fattureincloud-client#readme",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  },
  "type": "module",
  "module": "lib/index.js",
  "typings": "lib/index.d.ts",
  "exports": {
    ".": "./lib/index.js",
    "./customers": "./lib/customers/clients.js",
    "./customers/api": "./lib/customers/api.js",
    "./info": "./lib/info/clients.js",
    "./info/api": "./lib/info/api.js",
    "./invoices": "./lib/invoices/clients.js",
    "./invoices/api": "./lib/invoices/api.js",
    "./products": "./lib/products/clients.js",
    "./products/api": "./lib/products/api.js",
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "customers": [
        "./lib/customers/clients.d.ts"
      ],
      "customers/*": [
        "./lib/customers/*.d.ts"
      ],
      "info": [
        "./lib/info/clients.d.ts"
      ],
      "info/*": [
        "./lib/info/*.d.ts"
      ],
      "invoices": [
        "./lib/invoices/clients.d.ts"
      ],
      "invoices/*": [
        "./lib/invoices/*.d.ts"
      ],
      "products": [
        "./lib/products/clients.d.ts"
      ],
      "products/*": [
        "./lib/products/*.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "clean": "../../scripts/clean.mjs",
    "copy-dotfiles": "../../scripts/copy-dotfiles.mjs",
    "docs:help": "typedoc --showConfig",
    "docs:ae": "api-extractor run --config ./api-extractor.json --local --verbose",
    "docs:typedoc": "../../scripts/docs-typedoc.mjs",
    "docs": "run-s docs:ae docs:typedoc --print-label",
    "format": "prettier --config ../../config/prettier.cjs --write {__tests__,src}/**/*.{js,mjs,ts}",
    "lint": "eslint --config ../../config/eslint.cjs",
    "nuke": "npm run clean && rimraf node_modules 'package-lock.json'",
    "precommit": "lint-staged --config ../../config/lint-staged.cjs",
    "size": "pkg-size ./lib --sort-by=brotli --ignore-files {*.d.ts,*.map}",
    "test": "../../scripts/test.mjs",
    "test:ci": "../../scripts/test.mjs --ci",
    "test:watch": "npm run test -- --watch"
  },
  "dependencies": {
    "bottleneck": "^2.19.5",
    "debug": "^4.3.3",
    "phin": "^3.6.1"
  },
  "peerDependencies": {},
  "devDependencies": {}
}
