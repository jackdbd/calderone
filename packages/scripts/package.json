{
  "name": "@jackdbd/scripts",
  "version": "0.0.1",
  "description": "Development scripts",
  "author": "Giacomo Debidda <giacomo@giacomodebidda.com> (https://giacomodebidda.com/)",
  "license": "MIT",
  "private": true,
  "keywords": [
    "gcp",
    "scripts"
  ],
  "engines": {
    "node": ">=12.x",
    "npm": ">=7.0.0"
  },
  "type": "commonjs",
  "scripts": {
    "prebuild": "npm run clean",
    "build": "node build-scripts.mjs",
    "build:watch": "node build-scripts.mjs --watch",
    "clean": "rimraf coverage dist 'tsconfig.tsbuildinfo'",
    "format": "prettier --config ../../config/prettier.cjs --write {__tests__,src}/**/*.{js,mjs,ts}",
    "lint": "eslint --config ../../config/eslint.cjs",
    "nuke": "npm run clean && rimraf node_modules 'package-lock.json'",
    "precommit": "lint-staged --config ../../config/lint-staged.cjs",
    "prepublish:artifact-registry": "run-s clean build test",
    "publish:artifact-registry": "node ../../scripts/publish/artifact-registry.mjs --package-name checks",
    "pretest": "run-s build",
    "size": "pkg-size ./lib --sort-by=brotli",
    "test": "NODE_OPTIONS=--experimental-vm-modules npx jest --selectProjects checks --config ../../config/jest.cjs --rootDir ../../",
    "test:ci": "npm run test -- --ci --coverage",
    "test:watch": "npm run test -- --watch"
  },
  "dependencies": {
    "@google-cloud/containeranalysis": "3.3.0",
    "@google-cloud/pubsub": "2.19.0",
    "@google-cloud/tasks": "2.5.0",
    "ajv": "8.11.0",
    "debug": "4.3.4",
    "esbuild": "0.14.27",
    "googleapis": "97.0.0",
    "readline": "1.3.0",
    "twitter-api-v2": "1.11.1"
  },
  "peerDependencies": {},
  "devDependencies": {}
}
